{
  "expo": {
    "name": "Ticketfy",
    "slug": "Ticketfy",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mainpackage.ticketfy",
      "splash": {
        "backgroundColor": "#ffffff",
        "ldpi": "./assets/splash-ldpi-pt.png",
        "mdpi": "./assets/splash-mdpi-pt.png",
        "hdpi": "./assets/splash-hdpi-pt.png",
        "xhdpi": "./assets/splash-xhdpi-pt.png",
        "xxhdpi": "./assets/splash-xxhdpi-pt.png",
        "xxxhdpi": "./assets/splash-xxxhdpi-pt.png",
        "resizeMode": "contain"
      }
    },
    "android": {
      "package": "com.mainpackage.ticketfy",
      "versionCode": 2,
      "permissions": [],
      "icon": "./assets/icon.png",
      "splash": {
        "backgroundColor": "#ffffff",
        "ldpi": "./assets/splash-ldpi-pt.png",
        "mdpi": "./assets/splash-mdpi-pt.png",
        "hdpi": "./assets/splash-hdpi-pt.png",
        "xhdpi": "./assets/splash-xhdpi-pt.png",
        "xxhdpi": "./assets/splash-xxhdpi-pt.png",
        "xxxhdpi": "./assets/splash-xxxhdpi-pt.png",
        "resizeMode": "contain"
      }
    }
  }
}
